{
  "name": "b-zadanie-1-4",
  "version": "1.0.0",
  "description": "https://more2see.github.io/b-learning-git-2/",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:html": "globstar nu-html-checker *.html",
    "test:js": "globstar jshint js/*.js",
    "init-project": "npm install && npm-run-all init:*",
    "init:dirs": "mkdirp sass css vendor images js",
    "init:files": "touch README.md index.html sass/style.scss js/script.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm-run-all build:* test",
    "build:clean": "rimraf css/*.css",
    "build:sass": "node-sass --output-style compact -o css sass",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "build-dev:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "watch": "npm-run-all build -p watch:*",
    "watch:sassprefixer": "onchange sass/.scss -- npm-run all kompilator:sass -s autoprefixer",
    "kompilator:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "autoprefixer": "onchange -d 100 css/style.css -- autoprefixer-cli -o css/style.css css/style.css",
    "watch:browsersync": "browser-sync start --server --files css/style.css *.html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.23.7",
    "globstar": "^1.0.0",
    "jshint": "^2.9.5",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.2",
    "nu-html-checker": "^0.1.0",
    "onchange": "^3.3.0",
    "rimraf": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/more2see/b-learning-git-2.git"
  },
  "bugs": {
    "url": "https://github.com/more2see/b-learning-git-2/issues"
  },
  "homepage": "https://github.com/more2see/b-learning-git-2#readme",
  "dependencies": {}
}
